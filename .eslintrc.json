{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "commonjs": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "semi": "error",
        "quotes": ["error","double"] ,
        "camelcase":["error",{"properties": "always"}],
        "consistent-this": "error",
        "strict": ["error","global"],
        "react/jsx-no-undef": ["error",{"allowGlobals":true}],
        "react/prop-types": "off",
        "no-console":"off"
    },
    "plugins": [
        "react"
      ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
      ]
}